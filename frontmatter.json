{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Date",
          "name": "date",
          "type": "datetime",
          "dateFormat": "yyyy-MM-dd"
        },
        {
          "title": "Slug",
          "name": "slug",
          "editable": true,
          "type": "string"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "TranslationKey",
          "name": "translationKey",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.preview.host": "http://localhost:1313",
  "frontMatter.content.pageFolders": [
    {
      "title": "/en",
      "path": "[[workspace]]/content/en"
    },
    {
      "path": "[[workspace]]/content/en/post",
      "title": "/en/post"
    },
    {
      "title": "/zh-cn",
      "path": "[[workspace]]/content/zh-cn"
    },
    {
      "title": "/zh-cn/post",
      "path": "[[workspace]]/content/zh-cn/post"
    }
  ],
  "frontMatter.framework.startCommand": "./node_modules/hugo-extended/vendor/hugo server --buildDrafts --navigateToChanged",
  "frontMatter.custom.scripts": [
    {
      "title": "Rename (based on date and slug)",
      "id": "rename",
      "script": "./rename.js",
      "command": "node"
    }
  ]
}
