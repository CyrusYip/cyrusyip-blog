{
  "scripts": {
    "build": "hugo",
    "build:cloudflare": "git fetch --unshallow && hugo",
    "build:draft": "hugo --config no-comments.toml",
    "build:draft:cloudflare": "git fetch --unshallow && hugo --config no-comments.toml && cp robots.txt public/robots.txt",
    "clean": "rm --recursive public/",
    "dev": "hugo server --navigateToChanged",
    "dev:draft": "hugo server --navigateToChanged --config no-comments.toml"
  },
  "dependencies": {
    "hugo-extended": "0.124.1"
  }
}
