{
  "build": {
    "env": {
      "HUGO_VERSION": "0.99.1"
    }
  },
  "github": {
    "silent": true
  },
  "redirects": [
    {
      "source": "/post/2021/03/18/git-proxy-settings/",
      "destination": "/post/2021/03/24/configure-proxy/"
    },
    {
      "source": "/post/(index.xml)",
      "destination": "/zh-cn/$1"
    },
    {
      "source": "(/post/[0-9].*)",
      "destination": "/zh-cn$1"
    }
  ]
}
